s=input("Enter the time").split(":")
def palin(s:str):
    t=int(s)//10
    o=int(s)%10
    f=(o-t)*9+int(s)
    return f
f=palin(s[0])
g=int(s[0])+1
if(f==int(s[1])):
    f=palin(str(g))
    while(f>59):
        f=palin(str(g))
        g+=1
    if(f>10):
        print(f"{palin(str(f))}:{f}")
    else:
        print(f"{palin(str(f))}:0{f}")
else:
    print("00:00")